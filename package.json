{
  "name": "electron-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/electron-main/main.cjs",
  "scripts": {
    "serve": "concurrently \"npm run dev\" \"npm run start\" ",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "nodemon --exec electron . --watch ./ --ext .js,.cjs,.html,.scss,.vue,.ts,.css",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "electron:dist": "vue-tsc && vite build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ]
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@element-plus/icons-vue": "^2.1.0",
    "@types/node": "^18.15.0",
    "axios": "^1.3.4",
    "electron-is-dev": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "element-plus": "^2.2.36",
    "nodemon": "^2.0.21",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jridgewell/sourcemap-codec": "^1.4.14",
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^23.1.3",
    "eslint": "^8.35.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-order": "^6.0.3",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24",
    "web-storage-cache": "^1.1.1"
  },
  "lint-staged": {
    "*.{js,css,md,json,ts,vue}": "prettier --write",
    "*.less": "stylelint --fix"
  }
}
